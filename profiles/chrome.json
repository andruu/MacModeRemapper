{
  "name": "Chrome / Edge / Brave",
  "processNames": ["chrome", "msedge", "brave", "firefox", "opera"],
  "mappings": [
    { "trigger": "Alt+T", "action": "Ctrl+T" },
    { "trigger": "Alt+W", "action": "Ctrl+W" },
    { "trigger": "Alt+Shift+T", "action": "Ctrl+Shift+T" },
    { "trigger": "Alt+L", "action": "Ctrl+L" },
    { "trigger": "Alt+R", "action": "Ctrl+R" },
    { "trigger": "Alt+Shift+[", "action": "Ctrl+Shift+Tab" },
    { "trigger": "Alt+Shift+]", "action": "Ctrl+Tab" },
    { "trigger": "Alt+1", "action": "Ctrl+1" },
    { "trigger": "Alt+2", "action": "Ctrl+2" },
    { "trigger": "Alt+3", "action": "Ctrl+3" },
    { "trigger": "Alt+4", "action": "Ctrl+4" },
    { "trigger": "Alt+5", "action": "Ctrl+5" },
    { "trigger": "Alt+6", "action": "Ctrl+6" },
    { "trigger": "Alt+7", "action": "Ctrl+7" },
    { "trigger": "Alt+8", "action": "Ctrl+8" },
    { "trigger": "Alt+9", "action": "Ctrl+9" },
    { "trigger": "Alt+N", "action": "Ctrl+N" },
    { "trigger": "Alt+Shift+N", "action": "Ctrl+Shift+N" },
    { "trigger": "Alt+F", "action": "Ctrl+F" },
    { "trigger": "Alt+G", "action": "Ctrl+G" },
    { "trigger": "Alt+C", "action": "Ctrl+C" },
    { "trigger": "Alt+V", "action": "Ctrl+V" },
    { "trigger": "Alt+X", "action": "Ctrl+X" },
    { "trigger": "Alt+A", "action": "Ctrl+A" },
    { "trigger": "Alt+Z", "action": "Ctrl+Z" },
    { "trigger": "Alt+Shift+Z", "action": "Ctrl+Y" },
    { "trigger": "Alt+S", "action": "Ctrl+S" },
    { "trigger": "Alt+P", "action": "Ctrl+P" },
    { "trigger": "Alt+O", "action": "Ctrl+O" },
    { "trigger": "Alt+D", "action": "Ctrl+D" },
    { "trigger": "Alt+Q", "action": "special:close-window" },
    { "trigger": "Alt+Shift+V", "action": "Win+V" },
    { "trigger": "Alt+B", "action": "Ctrl+B" },
    { "trigger": "Alt+I", "action": "Ctrl+I" },
    { "trigger": "Alt+U", "action": "Ctrl+U" },
    { "trigger": "Alt+K", "action": "Ctrl+K" },

    { "trigger": "Alt+[", "action": "Alt+Left" },
    { "trigger": "Alt+]", "action": "Alt+Right" },

    { "trigger": "Alt+Left", "action": "Home" },
    { "trigger": "Alt+Right", "action": "End" },
    { "trigger": "Alt+Up", "action": "Ctrl+Home" },
    { "trigger": "Alt+Down", "action": "Ctrl+End" },
    { "trigger": "Alt+Shift+Left", "action": "Shift+Home" },
    { "trigger": "Alt+Shift+Right", "action": "Shift+End" },
    { "trigger": "Alt+Shift+Up", "action": "Ctrl+Shift+Home" },
    { "trigger": "Alt+Shift+Down", "action": "Ctrl+Shift+End" },
    { "trigger": "Alt+Backspace", "action": "Ctrl+Backspace" }
  ]
}
